---
<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
<link rel="self" type="application/atom+xml" href="{{ site.production_url }}/rss.xml"></link>
<channel>

  <title>{{ site.posts[0].title }}.</title>
  <link>{{ site.production_url }}{{ site.posts[0].url }}</link>
  <description>{{ site.title }}</description>

  <item>{{ site.posts[0].content | strip_html | truncate:200 }}</item>

</channel>
</rss>
